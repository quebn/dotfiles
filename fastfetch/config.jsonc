{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/master/doc/json_schema.json",
  "logo": {
    "type": "builtin",
    "source": "",
    "color": {
      "1": "cyan",
      "2": "green"
    },
    "width": null,
    "height": null,
    "padding": {
      "top": 0,
      "left": 1,
      "right": 2
    },
    "printRemaining": true,
    "preserveAspectRatio": false,
    "recache": false,
    "position": "left",
    "chafa": {
      "fgOnly": false,
      "symbols": "block+border+space-wide-inverted"
    }
  },
  "display": {
    "stat": false,
    "pipe": false,
    "showErrors": false,
    "disableLinewrap": true,
    "hideCursor": false,
    "separator": ": ",
    "color": {
      "keys": "white",
      "title": "",
      "output": "cyan",
      "separator": ""
    },
    "brightColor": true,
    "duration": {
      "abbreviation": false,
      "spaceBeforeUnit": "default"
    },
    "size": {
      "maxPrefix": "YB",
      "binaryPrefix": "iec",
      "ndigits": 2,
      "spaceBeforeUnit": "default"
    },
    "temp": {
      "unit": "D",
      "ndigits": 1,
      "color": {
        "green": "32",
        "yellow": "93",
        "red": "91"
      },
      "spaceBeforeUnit": "default"
    },
    "percent": {
      "type": [
        "num",
        "num-color"
      ],
      "ndigits": 0,
      "color": {
        "green": "32",
        "yellow": "93",
        "red": "91"
      },
      "spaceBeforeUnit": "default",
      "width": 0
    },
    "bar": {
      "char": {
        "elapsed": "■",
        "total": "-"
      },
      "border": {
        "left": "[ ",
        "right": " ]",
        "leftElapsed": "",
        "rightElapsed": ""
      },
      "color": {
        "elapsed": "auto",
        "total": "97",
        "border": "97"
      },
      "width": 10
    },
    "fraction": {
      "ndigits": 2
    },
    "noBuffer": false,
    "key": {
      "width": 0,
      "type": "icon",
      "paddingLeft": 0
    },
    "freq": {
      "ndigits": 2,
      "spaceBeforeUnit": "default"
    },
    "constants": []
  },
  "general": {
    "thread": true,
    "processingTimeout": 5000,
    "detectVersion": true,
    "playerName": "",
    "dsForceDrm": false
  },
  "modules": [
    {
      "type": "title",
      "key": "os",
      "keyIcon": "   ",
      "fqdn": false,
      "color": {
        "user": "blue",
        "at": "white",
        "host": "magenta"
      }
    },
    {
      "type": "separator",
      "string": "╭───────────── ソフトウェア ─────────────╮",
      "outputColor": "green",
      "times": 1
    },
    "break",
    {
      "type": "os",
      "keyIcon": "   "
    },
    {
      "type": "kernel",
      "keyIcon": "   "
    },
    {
      "type": "wm",
      "keyIcon": "   ",
      "detectPlugin": false
    },
    {
      "type": "shell",
      "keyIcon": "   "
    },
    {
      "type": "terminal",
      "keyIcon": "   "
    },
    {
      "type": "packages",
      "keyIcon": "  󰏖 ",
      "disabled": [
        "apk"
      ],
      "combined": false
    },
    {
      "type": "uptime",
      "keyIcon": "  󰔚 "
    },
    "break",
    {
      "type": "separator",
      "string": "├───────────── ハードウェア ─────────────┤",
      "outputColor": "green",
      "times": 1
    },
    "break",
    {
      "type": "host",
      "keyIcon": "   "
    },
    {
      "type": "display",
      "keyIcon": "   ",
      "compactType": "scaled-with-refresh-rate",
      "preciseRefreshRate": false
    },
    {
      "type": "cpu",
      "keyIcon": "   ",
      "temp": false,
      "showPeCoreCount": true
    },
    {
      "type": "gpu",
      "keyIcon": "  󰾲 ",
      "driverSpecific": false,
      "detectionMethod": "pci",
      "temp": false,
      "hideType": "none",
      "percent": {
        "green": 50,
        "yellow": 80,
        "type": 0
      }
    },
    {
      "type": "memory",
      "keyIcon": "   ",
      "format": "{used} / {total}",
      "percent": {
        "green": 50,
        "yellow": 80,
        "type": 0
      }
    },
    {
      "type": "disk",
      "keyIcon": "   ",
      "showRegular": true,
      "showExternal": true,
      "showHidden": true,
      "format": "{size-used} / {size-total}",
      "showSubvolumes": true,
      "showReadOnly": true,
      "showUnknown": false,
      "folders": "",
      "hideFolders": "/efi:/boot:/boot/efi:/boot/firmware",
      "hideFS": "",
      "useAvailable": false,
      "percent": {
        "green": 50,
        "yellow": 80,
        "type": 0
      }
    },
