    "break",
    {
      "type": "separator",
      "string": "╰────────────────────────────────────────╯",
      "outputColor": "green",
      "times": 1
    },
    {
      "type": "colors",
      "key": " ",
      "keyIcon": " ",
      "symbol": "circle",
      "paddingLeft": 13,
      "block": {
        "width": 4,
        "range": [
          0,
          15
        ]
      }
    }
  ]
